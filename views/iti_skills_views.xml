<odoo>
    <data>

        <record id="skills_action" model="ir.actions.act_window">
            <field name="name">Skills Action</field>
            <field name="res_model">iti.skills</field>
        </record>

        <record id="skills_tree_view" model="ir.ui.view">
            <field name="name">Skills Tree View</field>
            <field name="model">iti.skills</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

         <record id="skills_form_view" model="ir.ui.view">
            <field name="name">Skills Form View</field>
            <field name="model">iti.skills</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                    </group>
                </form>
            </field>
        </record>

        <menuitem id="skills_menu_item" name="Skills" parent="student_menu_item" action="skills_action"/>
    </data>
</odoo>